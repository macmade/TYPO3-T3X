<?xml version="1.0" encoding="utf-8" standalone="yes" ?>

<!-- $Id$ -->

<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
        <langChildren>0</langChildren>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:twitter_login/lang/flex_pi1.xml:sDEF</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <storagePage>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:twitter_login/lang/flex_pi1.xml:sDEF.storagePage</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <show_thumbs>1</show_thumbs>
                                <size>1</size>
                                <minitems>1</minitems>
                                <maxitems>1</maxitems>
                            </config>
                        </TCEforms>
                    </storagePage>
                    <userGroup>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:twitter_login/lang/flex_pi1.xml:sDEF.userGroup</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>fe_groups</allowed>
                                <show_thumbs>1</show_thumbs>
                                <size>1</size>
                                <minitems>1</minitems>
                                <maxitems>1</maxitems>
                            </config>
                        </TCEforms>
                    </userGroup>
                </el>
            </ROOT>
        </sDEF>
        <sOAUTH>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:twitter_login/lang/flex_pi1.xml:sOAUTH</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <consumerKey>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:twitter_login/lang/flex_pi1.xml:sOAUTH.consumerKey</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </consumerKey>
                    <consumerSecret>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:twitter_login/lang/flex_pi1.xml:sOAUTH.consumerSecret</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </consumerSecret>
                    <callback>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:twitter_login/lang/flex_pi1.xml:sOAUTH.callback</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </callback>
                    <afterLoginRedirect>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:twitter_login/lang/flex_pi1.xml:sOAUTH.afterLoginRedirect</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <show_thumbs>1</show_thumbs>
                                <size>1</size>
                                <minitems>1</minitems>
                                <maxitems>1</maxitems>
                            </config>
                        </TCEforms>
                    </afterLoginRedirect>
                </el>
            </ROOT>
        </sOAUTH>
    </sheets>
</T3DataStructure>
