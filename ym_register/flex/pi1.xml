<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
        <langChildren>0</langChildren>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:ym_register/lang/flex_pi1.xml:sDEF</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <storagePage>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:ym_register/lang/flex_pi1.xml:sDEF.storagePage</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <show_thumbs>1</show_thumbs>
                                <size>1</size>
                                <minitems>1</minitems>
                                <maxitems>1</maxitems>
                            </config>
                        </TCEforms>
                    </storagePage>
                    <userGroup>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:ym_register/lang/flex_pi1.xml:sDEF.userGroup</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>fe_groups</allowed>
                                <show_thumbs>1</show_thumbs>
                                <size>1</size>
                                <minitems>1</minitems>
                                <maxitems>1</maxitems>
                            </config>
                        </TCEforms>
                    </userGroup>
                    <terms>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:ym_register/lang/flex_pi1.xml:sDEF.terms</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <show_thumbs>1</show_thumbs>
                                <size>1</size>
                                <minitems>1</minitems>
                                <maxitems>1</maxitems>
                            </config>
                        </TCEforms>
                    </terms>
                </el>
            </ROOT>
        </sDEF>
        <sMAILER>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:ym_register/lang/flex_pi1.xml:sMAILER</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <subject>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:ym_register/lang/flex_pi1.xml:sMAILER.subject</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </subject>
                    <fromName>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:ym_register/lang/flex_pi1.xml:sMAILER.fromName</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </fromName>
                    <from>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:ym_register/lang/flex_pi1.xml:sMAILER.from</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </from>
                    <replyTo>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:ym_register/lang/flex_pi1.xml:sMAILER.replyTo</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </replyTo>
                    <message>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:ym_register/lang/flex_pi1.xml:sMAILER.message</label>
                            <config>
                                <type>text</type>
                                <cols>30</cols>
                                <rows>10</rows>
                            </config>
                        </TCEforms>
                    </message>
                </el>
            </ROOT>
        </sMAILER>
    </sheets>
</T3DataStructure>
