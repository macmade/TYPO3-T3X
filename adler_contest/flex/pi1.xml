                    <subject>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:adler_contest/lang/flex_pi1.xml:sMAILER.subject</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </subject>
                    <message>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:adler_contest/lang/flex_pi1.xml:sMAILER.message</label>
                            <config>
                                <type>text</type>
                                <rows>5</rows>
                                <cols>30</cols>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </message>
                </el>
            </ROOT>
        </sMAILER>
        <sPROFILE>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:adler_contest/lang/flex_pi1.xml:sPROFILE</sheetTitle>
