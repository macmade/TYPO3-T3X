<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
        <langChildren>0</langChildren>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:adler_contest/lang/flex_pi1.xml:sDEF</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <pages>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:adler_contest/lang/flex_pi1.xml:sDEF.pages</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <show_thumbs>1</show_thumbs>
                                <size>1</size>
                                <minitems>1</minitems>
                                <maxitems>1</maxitems>
                            </config>
                        </TCEforms>
                    </pages>
                    <group>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:adler_contest/lang/flex_pi1.xml:sDEF.group</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>fe_groups</allowed>
                                <show_thumbs>1</show_thumbs>
                                <size>1</size>
                                <minitems>1</minitems>
                                <maxitems>1</maxitems>
                            </config>
                        </TCEforms>
                    </group>
                    <info_page>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:adler_contest/lang/flex_pi1.xml:sDEF.info_page</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <show_thumbs>1</show_thumbs>
                                <size>1</size>
                                <minitems>1</minitems>
                                <maxitems>1</maxitems>
                            </config>
                        </TCEforms>
                    </info_page>
                </el>
            </ROOT>
        </sDEF>
        <sREGISTER>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:adler_contest/lang/flex_pi1.xml:sREGISTER</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <header>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:adler_contest/lang/flex_pi1.xml:sREGISTER.header</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </header>
                    <description>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:adler_contest/lang/flex_pi1.xml:sREGISTER.description</label>
                            <config>
                                <type>text</type>
                                <rows>5</rows>
                                <cols>30</cols>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </description>
                    <conditions>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:adler_contest/lang/flex_pi1.xml:sREGISTER.conditions</label>
                            <config>
                                <type>group</type>
                                <internal_type>file</internal_type>
                                <allowed>pdf</allowed>
                                <max_size>2048</max_size>
                                <uploadfolder>uploads/tx_adlercontest</uploadfolder>
                                <show_thumbs>1</show_thumbs>
                                <size>1</size>
                                <minitems>1</minitems>
                                <maxitems>1</maxitems>
                            </config>
                        </TCEforms>
                    </conditions>
                    <confirmation>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:adler_contest/lang/flex_pi1.xml:sREGISTER.confirmation</label>
                            <config>
                                <type>text</type>
                                <rows>5</rows>
                                <cols>30</cols>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </confirmation>
                </el>
            </ROOT>
        </sREGISTER>
        <sMAILER>
            <ROOT>
                <TCEforms>
