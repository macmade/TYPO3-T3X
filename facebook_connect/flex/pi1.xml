                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <show_thumbs>1</show_thumbs>
                                <size>1</size>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                            </config>
                        </TCEforms>
                    </onLogin>
                    <afterLogin>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:facebook_connect/lang/flex_pi1.xml:sREDIRECT.afterLogin</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <show_thumbs>1</show_thumbs>
                                <size>1</size>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                            </config>
                        </TCEforms>
                    </afterLogin>
                </el>
