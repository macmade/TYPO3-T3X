                    </userGroup>
                    <appKey>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:facebook_connect/lang/flex_pi1.xml:sDEF.appKey</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </appKey>
                    <appSecret>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:facebook_connect/lang/flex_pi1.xml:sDEF.appSecret</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </appSecret>
                    <noNewUserMessage>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:facebook_connect/lang/flex_pi1.xml:sDEF.noNewUserMessage</label>
                            <config>
                                <type>check</type>>
                            </config>
                        </TCEforms>
                    </noNewUserMessage>
                </el>
            </ROOT>
        </sDEF>
        <sREDIRECT>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:facebook_connect/lang/flex_pi1.xml:sREDIRECT</sheetTitle>
                </TCEforms>
