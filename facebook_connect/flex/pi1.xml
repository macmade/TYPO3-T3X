<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
        <langChildren>0</langChildren>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:facebook_connect/lang/flex_pi1.xml:sDEF</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <pages>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:facebook_connect/lang/flex_pi1.xml:sDEF.pages</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <show_thumbs>1</show_thumbs>
                                <size>1</size>
                                <minitems>1</minitems>
                                <maxitems>1</maxitems>
                            </config>
                        </TCEforms>
                    </pages>
                    <userGroup>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:facebook_connect/lang/flex_pi1.xml:sDEF.userGroup</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>fe_groups</allowed>
                                <show_thumbs>1</show_thumbs>
                                <size>1</size>
                                <minitems>1</minitems>
                                <maxitems>1</maxitems>
                            </config>
