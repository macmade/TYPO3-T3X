<?xml version="1.0" encoding="utf-8" standalone="yes" ?>

<!-- $Id: pi1.xml 190 2010-01-06 11:16:55Z macmade $ -->

<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
        <langChildren>0</langChildren>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:marvin_catalog/lang/flex-pi1.xml:sDEF</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <rootCategory>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:marvin_catalog/lang/flex-pi1.xml:sDEF.rootCategory</label>
                            <config>
                                <type>select</type>
                                <size>1</size>
                                <maxItems>1</maxItems>
                                <itemsProcFunc>tx_marvincatalog_Flexform_Fields->getMagentoRootCategories</itemsProcFunc>
                            </config>
                        </TCEforms>
                    </rootCategory>
                    <disableZoom>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:marvin_catalog/lang/flex-pi1.xml:sDEF.disableZoom</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </disableZoom>
                </el>
            </ROOT>
        </sDEF>
        <sBITLY>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:marvin_catalog/lang/flex-pi1.xml:sBITLY</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <login>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:marvin_catalog/lang/flex-pi1.xml:sBITLY.login</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                            </config>
                        </TCEforms>
                    </login>
                    <apiKey>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:marvin_catalog/lang/flex-pi1.xml:sBITLY.apiKey</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                            </config>
                        </TCEforms>
                    </apiKey>
                </el>
            </ROOT>
        </sBITLY>
        <sMAIL>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:marvin_catalog/lang/flex-pi1.xml:sMAIL</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <subject>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:marvin_catalog/lang/flex-pi1.xml:sMAIL.subject</label>
                            <config>
                                <type>input</type>
                                <size>30</size>
                            </config>
                        </TCEforms>
                    </subject>
                    <message>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:marvin_catalog/lang/flex-pi1.xml:sMAIL.message</label>
                            <config>
                                <type>text</type>
                                <cols>30</cols>
                                <rows>5</rows>
                            </config>
                        </TCEforms>
                    </message>
                </el>
            </ROOT>
        </sMAIL>
    </sheets>
</T3DataStructure>
